<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>Trm</from-outcome>
            <to-view-id>/Vistas/Trm/Trm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Regalias</from-outcome>
            <to-view-id>/Vistas/Regalias/Regalias.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ConsultarRegalias</from-outcome>
            <to-view-id>/Vistas/Regalias/ConsultarRegalias.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
        <navigation-case>
            <from-outcome>Pbl</from-outcome>
            <to-view-id>/Vistas/Pbl/Pbl.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Contrato</from-outcome>
            <to-view-id>/Vistas/Contratos/Contratos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Campos</from-outcome>
            <to-view-id>/Vistas/Campos/Campos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Municipios</from-outcome>
            <to-view-id>/Vistas/Departamentos_municipios/Municipios.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Resultado</from-outcome>
            <to-view-id>/Vistas/Resultado.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Departamentos</from-outcome>
            <to-view-id>/Vistas/Departamentos_municipios/Departamentos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>CalidadCrudos</from-outcome>
            <to-view-id>/Vistas/Pbl/CalidadCrudos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>Derivados</from-outcome>
            <to-view-id>/Vistas/Pbl/Derivados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Valores</from-outcome>
            <to-view-id>/Vistas/Pbl/Valores.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Dashboard</from-outcome>
            <to-view-id>/Vistas/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>departamentoCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.DepartamentoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>contratoCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.ContratoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>municipioCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.MunicipioCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>campoCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.CampoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>tipoCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.TipoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>produccionCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.ProduccionCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pblCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.PblCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>regaliasCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.RegaliasCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>trmCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.TrmCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>valoresCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.ValoresCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>calidadCrudoCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.CalidadCrudoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ponderadoRefinacionCT</managed-bean-name>
        <managed-bean-class>com.planit.scr.controladores.PonderadoRefinacionCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
